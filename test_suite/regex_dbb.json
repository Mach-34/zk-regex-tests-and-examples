{
  "bench_all": true,
  "database": [
    {
      "circuit_name": "subject_all",
      "regex": {
        "decomposed": [
          {
            "is_public": false,
            "regex_def": "(\\r\\n|^)subject:"
          },
          {
            "is_public": true,
            "regex_def": "[^\\r\\n]+"
          },
          {
            "is_public": false,
            "regex_def": "\\r\\n"
          }
        ]
      },
      "gen_substrs": false,
      "input_size": 1024,
      "samples_pass": [
        {
          "input": "subject:This is a test.\r\n",
          "expected_substrings": ["This is a test."],
          "circom_testname": "subject from beginning"
        },
        {
          "input": "dummy\r\nsubject:This is a test.\r\n",
          "expected_substrings": ["This is a test."],
          "circom_testname": "subject after new line"
        },
        {
          "input": "subject:これはテストです。\r\n",
          "expected_substrings": ["これはテストです。"],
          "circom_testname": "subject from beginning and non-English case"
        }
      ],
      "samples_fail": [
        "\r\nto:subject:This is a subject in To field.\r\n",
        "\u00FF\u0031subject:This is a subject in To field.\r\n"
      ],
      "circom_testnames_fails": [
        "subject in the invalid field",
        "invalid subject field with 255"
      ]
    }
  ]
}
